{
  "scripts": {
    "build-dev": "node webpack/webpack-build.js",
    "build-dev-eval": "node webpack/webpack-build.js --sourceMapEval",
    "build-prod": "node webpack/webpack-build.js -p",
    "analyse": "node webpack/webpack-build.js -p --save-json && package-size-analyzer -j webpack/webpack-stats.json",
    "postinstall": "node ./scripts/install-githooks.js && node ./scripts/setup-eslint-codes",
    "eslint": "eslint --ext .js,.jsx client/app",
    "eslint-cmd": "eslint",
    "pre-commit": "node ./scripts/pre-commit-eslint.js",
    "prepare-locale-json": "node ./scripts/prepare-locale-json.js",
    "gen-sprites": "node ./scripts/build-sprites.js",
    "bk-build-dev-debug": "npm run gen-sprites && fis3 release dev -cwf ./fis-conf-base.js  -d ../server --verbose",
    "bk-build-qa": "npm run gen-sprites && fis3 release qa -cwf ./fis-conf.js -d ../server/public",
    "bk-build-prod": "npm run gen-sprites && fis3 release prod -cwf ./fis-conf.js -d ../release"
  },
